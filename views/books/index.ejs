<%- include('../partials/header') %>

    <table id='list'>
    <thead>
      <tr>
        <th>Title</th>
        <th>Author</th>
      </tr>
    </thead>
    <tbody>
      <% books.forEach((b) => { %>
        <tr>
        <td><%= b.title %></td>
        <td><%= b.author %></td>

        <td><a href="/books/<%= b._id %>">REVIEWS</a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>

  <%- include('../partials/footer') %>